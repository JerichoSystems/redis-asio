{
    "name": "redis-asio",
    "version-semver": "0.1.5",
    "description": "Redis client on Boost.Asio with awaitable API",
    "homepage": "https://github.com/jerichosystems/redis_asio",
    "dependencies": [
        {
            "name": "boost-asio",
            "version>=": "1.88.0"
        },
        {
            "name": "hiredis",
            "features": ["ssl"],
            "version>=": "1.3.0"
        }
    ],
    "features": {
        "tests": {
            "description": "Build tests (not run during port build)",
            "dependencies": [
                {
                    "name": "gtest",
                    "host": true,
                    "version>=": "1.17.0"
                }
            ]
        },
        "examples": {
            "description": "Build examples"
        },
        "sanitize": {
            "description": "Enable Address/UB sanitizers"
        },
        "benchmarks": {
            "description": "Build benchmarks using Google Benchmark",
            "dependencies": [
                {
                    "name": "benchmark",
                    "host": true,
                    "version>=": "1.9.4"
                }
            ]
        }
    },
    "builtin-baseline": "16c71a39e5a0fc0bdb3fad03beef8f38ee00ee3b"
}
